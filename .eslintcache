  required\n            label=\"Email address\"\n            htmlFor=\"emailAddress-input-personal\"\n            name=\"emailAddress\"\n            validate={emailValidation}\n          >\n            <MaskedInput\n              type=\"email\"\n              mask={emailMask}\n              id=\"emailAddress-input-personal\"\n              name=\"emailAddress\"\n            />\n          </FormField>\n\n          <Box\n            align={size !== \"small\" ? \"start\" : undefined}\n            margin={{ top: \"medium\", bottom: \"small\" }}\n            direction=\"row\"\n            gap=\"small\"\n          >\n            <Button label=\"Cancel\" secondary onClick={() => setOpen(false)} />\n            <Button\n              label=\"Confirm\"\n              primary\n              disabled={preliminaryValidation || disabled}\n              type=\"submit\"\n            />\n          </Box>\n        </Form>\n      </Box>\n    </Box>\n  );\n};\n\nexport default EditProfile;\n","/Users/mahmedtan/Developer/derm-app/src/reducers/selectedItemReducer.js",[],["494","495"],"/Users/mahmedtan/Developer/derm-app/src/services/consultations.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Services/Service.js",["496","497","498"],"import React, { useContext, useEffect } from \"react\";\nimport { Box, Heading, Card, Button, ResponsiveContext } from \"grommet\";\nimport Loading from \"../Extras/Loading\";\nimport BlockContentMain from \"../../components/Utils/BlockContentMain\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { selectItem } from \"../../reducers/selectedItemReducer\";\nimport Footer from \"../../components/Footer/Footer\";\nimport PageNotFound from \"../Extras/404\";\nimport SidebarAccordian from \"../../components/Sidebars/SidebarAccordian\";\nimport { LinkNext } from \"grommet-icons\";\nimport { Link } from \"react-router-dom\";\nimport FaceVisual from \"../../components/FaceVisual/FaceVisual\";\n\nconst Service = ({ service, services, servi