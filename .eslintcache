Link>\n                  <Link to=\"/specials\" onClick={removeLayer}>\n                    <Button>\n                      <Heading level=\"3\" margin=\"none\">\n                        Specials\n                      </Heading>\n                    </Button>\n                  </Link>\n                  <Link to=\"/services\" onClick={removeLayer}>\n                    <Button>\n                      <Heading level=\"3\" margin=\"none\">\n                        Services\n                      </Heading>\n                    </Button>\n                  </Link>\n                  <Link to=\"/finance\" onClick={removeLayer}>\n                    <Button>\n                      <Heading level=\"3\" margin=\"none\">\n                        Finance\n                      </Heading>\n                    </Button>\n                  </Link>\n                  <Link to=\"/contact-us\" onClick={removeLayer}>\n                    <Button>\n                      <Heading level=\"3\" margin=\"none\">\n                        Contact us\n                      </Heading>\n                    </Button>\n                  </Link>\n                  <Link to=\"/About-us\" onClick={removeLayer}>\n                    <Button>\n                      <Heading level=\"3\" margin=\"none\">\n                        About Us\n                      </Heading>\n                    </Button>\n                  </Link>\n                </Box>\n              </Box>\n              <Box\n                alignSelf=\"center\"\n                onClick={() =>\n                  document.body.classList.remove(\"is-scrollLocked\")\n                }\n              >\n                <AuthenticationBtn />\n              </Box>\n            </Box>\n          </Layer>\n        )}\n      </Box>\n    </Box>\n  );\n};\n\nexport default Snackbar;\n",["439","440"],"/Users/mahmedtan/Developer/derm-app/src/components/Header/Navbar.js",["441"],"/Users/mahmedtan/Developer/derm-app/src/components/Authentication/