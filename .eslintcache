xtAlign=\"center\"\n      >\n        Booking for{\" \"}\n        {new Date(form.bookedFor).toLocaleDateString(\"en-US\", {\n          month: \"long\",\n          day: \"numeric\",\n        })}\n      </Text>\n\n      <Box\n        align={size === \"small\" ? \"start\" : \"center\"}\n        fill\n        pad={{ horizontal: \"xlarge\" }}\n      >\n        <Box gap=\"medium\">\n          <Box gap=\"medium\" direction=\"row-responsive\" justify=\"center\">\n            <Box width=\"small\" align={alignTitle}>\n              <Text weight=\"bold\" size=\"large\">\n                First Name\n              </Text>\n              {form.firstName}\n            </Box>\n            <Box width=\"small\">\n              <Text weight=\"bold\" size=\"large\">\n                Last Name\n              </Text>\n              {form.lastName}\n            </Box>\n          </Box>\n\n          <Box gap=\"medium\" direction=\"row-responsive\" justify=\"center\">\n            <Box width=\"small\" align={alignTitle}>\n              <Text weight=\"bold\" size=\"large\">\n                Email Address\n              </Text>\n              {form.emailAddress}\n            </Box>\n            <Box width=\"small\">\n              <Text weight=\"bold\" size=\"large\">\n                Date & Time\n              </Text>\n              {new Date(form.bookedFor).toLocaleDateString(\"en-US\", {\n                year: \"numeric\",\n                month: \"short\",\n                day: \"numeric\",\n              }) +\n                \" \" +\n                new Date(form.bookedFor).toLocaleTimeString(\"en-US\", {\n                  timeStyle: \"short\",\n                })}\n            </Box>\n          </Box>\n          <Box gap=\"medium\" direction=\"row-responsive\" justify=\"center\">\n            <Box width=\"small\" align={alignTitle}>\n              <Text weight=\"bold\" size=\"large\">\n                Phone Number\n              </Text>\n              {form.phoneNumber}\n            </Box>\n            <Box width=\"small\">\n              <Text weight=\"bold\" size=\"large\">\n                Submitted at\n              </Text>\n              {new Date(form.submitted).toLocaleDateString(\"en-US\", {\n                year: \"numeric\",\n                month: \"short\",\n                day: \"numeric\",\n              }) +\n                \" \" +\n                new Date(form.submitted).toLocaleTimeString(\"en-US\", {\n                  timeStyle: \"short\",\n                })}\n            </Box>\n          </Box>\n        </Box>\n      </Box>\n\n      <Consultations title=\"Consultations\" consultations={form.consultations} />\n      <Consultations title=\"Procedures\" consultations={form.procedures} />\n\n      {form.images.length > 0 && (\n        <Box gap=\"medium\">\n          <Text\n            size=\"xlarge\"\n            weight=\"normal\"\n            margin=\"medium\"\n            alignSelf=\"center\"\n          >\n            Images\n          </Text>\n          <Box gap=\"large\" direction=\"row-responsive\">\n            {form.images &&\n              form.images.map((url) => (\n                <Box key={url} width=\"small\" border round overflow=\"hidden\">\n                  <Image src={url} />\n                </Box>\n              ))}\n          </Box>\n        </Box>\n      )}\n      {form.remarks && (\n        <Box>\n          <Text\n            size=\"xlarge\"\n            weight=\"normal\"\n            margin=\"medium\"\n            alignSelf=\"center\"\n          >\n            Notes\n          </Text>\n          <Paragraph textAlign=\"center\">{form.remarks}</Paragraph>\n        </Box>\n      )}\n\n      {form.cancelled ? (\n        <Button\n          disabled\n          label=\"Cancelled\"\n          color=\"status-critical\"\n          style={{ color: \"white\" }}\n        />\n      ) : (\n        <Button primary disabled={open} onClick={() => setOpen(true)}>\n          Cancel Appointment\n        </Button>\n      )}\n      {open && (\n        <Box gap=\"small\">\n          <Box direction=\"row\" align=\"start\" margin={{ vertical: \"medium\" }}>\n            <CheckBox\n              checked={checkBoxCancel}\n              onChange={(e) => setCheckBoxCancel(e.target.checked)}\n            />\n\n            <Text alignSelf=\"center\">\n              We ask that you give us at least 24 hours notice if you need to\n              cancel or reschedule an appointment.\n            </Text>\n          </Box>\n\n          <Box direction=\"row\" gap=\"medium\" justify=\"center\">\n            <Button\n              label=\"Cancel & Reschedule\"\n              onClick={() => {\n                setDisabled(true);\n                deleteAndReschedule(\n                  form._id,\n                  form.emailAddress,\n                  form.bookedFor,\n                  form.firstName + \" \" + form.lastName\n                );\n              }}\n              primary\n              disabled={!checkBoxCancel || disabled}\n            />\n            <Button\n              label=\"Just Cancel\"\n              primary\n              color=\"status-critical\"\n              onClick={() => {\n                setDisabled(true);\n\n                deleteForm(\n                  form._id,\n                  form.emailAddress,\n                  form.bookedFor,\n                  form.firstName + \" \" + form.lastName\n                );\n              }}\n              disabled={!checkBoxCancel || disabled}\n            />\n          </Box>\n        </Box>\n      )}\n    </Box>\n  );\n};\n\nexport default Appointment;\n","/Users/mahmedtan/Developer/derm-app/src/reducers/serviceTypeReducer.js",[],"/Users/mahmedtan/Developer/derm-app/src/services/procedures.js",[],["484","485"],"/Users/mahmedtan/Developer/derm-app/src/pages/Book/StepFooter.js",[],"/Users/mahmedtan/Developer/derm-app/src/components/Utils/BlockContentMain.js",[],["486","487"],"/Users/mahmedtan/Developer/derm-app/src/components/Sidebars/