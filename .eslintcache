laceholder: \"com\",\n  },\n];\nconst phoneMask = [\n  { fixed: \"(\" },\n  {\n    length: 3,\n    regexp: /^[0-9]{1,3}$/,\n    placeholder: \"xxx\",\n  },\n  { fixed: \")\" },\n  { fixed: \" \" },\n  {\n    length: 3,\n    regexp: /^[0-9]{1,3}$/,\n    placeholder: \"xxx\",\n  },\n  { fixed: \"-\" },\n  {\n    length: 4,\n    regexp: /^[0-9]{1,4}$/,\n    placeholder: \"xxxx\",\n  },\n];\n\nexport const EditProfile = ({\n  emailAddress,\n  fullName,\n  phoneNumber,\n  setChange,\n  _id,\n  setMessage,\n  patientId,\n  sub,\n}) => {\n  const [formValues, setFormValues] = React.useState({\n    emailAddress: emailAddress || \"\",\n    fullName: fullName || \"\",\n    phoneNumber: phoneNumber || \"\",\n  });\n\n  const [open, setOpen] = useState(false);\n  const [disabled, setDisabled] = useState(true);\n\n  const preliminaryValidation = !(\n    formValues.fullName.length >= 3 &&\n    new RegExp(\"^\\\\(?([0-9]{3})\\\\) ?[-.●]?([0-9]{3})[-.●]?([0-9]{4})$\").test(\n      formValues.phoneNumber\n    ) &&\n    new RegExp(\"[^@ \\\\t\\\\r\\\\n]+@[^@ \\\\t\\\\r\\\\n]+\\\\.[^@ \\\\t\\\\r\\\\n]+\").test(\n      formValues.emailAddress\n    )\n  );\n  const size = useContext(ResponsiveContext);\n\n  const onChange = (values) => {\n    setDisabled(false);\n    setFormValues(values);\n  };\n\n  // eslint-disable-next-line no-unused-vars\n  const onSubmit = ({ value, touched }) => {\n    setOpen(false);\n    updatePatientDetails({\n      ...value,\n      fullName: fullName || value.fullName,\n      sub,\n      id: _id || uuidv4(),\n      patientId: patientId || uuidv4().substring(0, 8),\n    }).then((res, error) => {\n      setMessage(\"Your Profile has been updated\");\n      console.log(res);\n      setChange();\n    });\n  };\n  if (!open)\n    return (\n      <Button\n        label=\"Edit Profile\"\n        primary\n        onClick={() => {\n          setOpen(true);\n        }}\n      />\n    );\n  return (\n    <Box gap=\"medium\" align=\"center\" animation=\"slideDown\">\n      <Text size=\"xlarge\" weight=\"normal\" margin=\"small\">\n        Edit Profile\n      </Text>\n\n      <Box pad={{ horizontal: \"xxsmall\" }} fill>\n        <Form\n          validate=\"blur\"\n          value={formValues}\n          onChange={(nextValue) => onChange(nextValue)}\n          onSubmit={({ value, touched }) => onSubmit({ value, touched })}\n        >\n          {!fullName && (\n            <FormField\n              label=\"Full Name\"\n              htmlFor=\"fullName-input-persona\"\n              name=\"fullName\"\n              validate={(value) =>\n                value.length < 3 && \"Name must at least contain 3 letters \"\n              }\n              required\n            >\n              <TextInput\n                id=\"fullName-input-personal\"\n                name=\"fullName\"\n                placeholder=\"Jane Smith\"\n              />\n            </FormField>\n          )}\n\n          <FormField\n            label=\"Phone number\"\n            htmlFor=\"phoneNumber-input-personal\"\n            name=\"phoneNumber\"\n            validate={phoneNumberValidation}\n            required\n          >\n            <MaskedInput\n              placeholder=\"(xxx) xxx-xxxx\"\n              type=\"tel\"\n              mask={phoneMask}\n              id=\"phoneNumber-input-personal\"\n              name=\"phoneNumber\"\n            />\n          </FormField>\n          <FormField\n            required\n            label=\"Email address\"\n            htmlFor=\"emailAddress-input-personal\"\n            name=\"emailAddress\"\n            validate={emailValidation}\n          >\n            <MaskedInput\n              type=\"email\"\n              mask={emailMask}\n              id=\"emailAddress-input-personal\"\n              name=\"emailAddress\"\n            />\n          </FormField>\n\n          <Box\n            align={size !== \"small\" ? \"start\" : undefined}\n            justify=\"center\"\n            margin={{ top: \"medium\", bottom: \"small\" }}\n            direction=\"row\"\n            gap=\"small\"\n          >\n            <Button label=\"Cancel\" secondary onClick={() => setOpen(false)} />\n            <Button\n              label=\"Confirm\"\n              primary\n              disabled={preliminaryValidation || disabled}\n              type=\"submit\"\n            />\n          </Box>\n        </Form>\n      </Box>\n    </Box>\n  );\n};\n\nexport default EditProfile;\n","/Users/mahmedtan/Developer/derm-app/src/reducers/selectedItemReducer.js",[],"/Users/mahmedtan/Developer/derm-app/src/services/consultations.js",[],"/Users/mahmedtan/Developer/derm-app/src/services/patients.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Patient/PatientOld.js",[],["498","499"],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Patient/DetailsField.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/VerifyBanner.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Appointments/Appointments.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Appointments/Appointment.js",["500"],"import {\n  Card,\n  Box,\n  Text,\n  Image,\n  Button,\n  Paragraph,\n  ResponsiveContext,\n  CheckBox,\n} from \"grommet\";\nimport React, { useContext, useState } from \"react\";\nimport Consultations from \"./Consult\";\n\nconst Appointment = ({ form, deleteForm, deleteAndReschedule }) => {\n  const [open, setOpen] = useState(false);\n  const [disabled, setDisabled] = useState(false);\n  const size = useContext(ResponsiveContext);\n  const alignTitle = size === \"small\" ? \"start\" : \"end\";\n  const [checkBoxCancel, setCheckBoxCancel] = useState(false);\n\n  return (\n    <Box\n      align=\"center\"\n      gap=\"medium\"\n      margin=\"medium\"\n      animation=\"fadeIn\"\n      width=\"large\"\n    >\n      <Text\n        size=\"xlarge\"\n        weight=\"normal\"\n        margin=\"medium\"\n        alignSelf=\"center\"\n        textAlign=\"center\"\n      >\n        Booking for{\" \"}\n        {new Date(form.bookedFor).toLocaleDateString(\"en-US\", {\n          month: \"long\",\n          day: \"numeric\",\n        })}\n      </Text>\n\n      <Box\n        align={size === \"small\" ? \"start\" : \"center\"}\n        fill\n        pad={{ horizontal: \"xlarge\" }}\n      >\n        <Box gap=\"medium\">\n          <Box gap=\"medium\" direction=\"row-responsive\" justify=\"center\">\n            <Box width=\"small\" align={alignTitle}>\n              <Text weight=\"bold\" size=\"large\">\n                First Name\n              </Text>\n              {form.firstName}\n            </Box>\n            <Box width=\"small\">\n              <Text weight=\"bold\" size=\"large\">\n                Last Name\n              </Text>\n              {form.lastName}\n            </Box>\n          </Box>\n\n          <Box gap=\"medium\" direction=\"row-responsive\" justify=\"center\">\n            <Box width=\"small\" align={alignTitle}>\n              <Text weight=\"bold\" size=\"large\">\n                Email Address\n              </Text>\n              {form.emailAddress}\n            </Box>\n            <Box width=\"small\">\n              <Text weight=\"bold\" size=\"large\">\n                Date & Time\n              </Text>\n              {new Date(form.bookedFor).toLocaleDateString(\"en-US\", {\n                year: \"numeric\",\n                month: \"short\",\n                day: \"numeric\",\n              }) +\n                \" \" +\n                new Date(form.bookedFor).toLocaleTimeString(\"en-US\", {\n                  timeStyle: \"short\",\n                })}\n            </Box>\n          </Box>\n          <Box gap=\"medium\" direction=\"row-responsive\" justify=\"center\">\n            <Box width=\"small\" align={alignTitle}>\n              <Text weight=\"bold\" size=\"large\">\n                Phone Number\n              </Text>\n              {form.phoneNumber}\n            </Box>\n            <Box width=\"small\">\n              <Text weight=\"bold\" size=\"large\">\n                Submitted at\n              </Text>\n              {new Date(form.submitted).toLocaleDateString(\"en-US\", {\n                year: \"numeric\",\n                month: \"short\",\n                day: \"numeric\",\n              }) +\n                \" \" +\n                new Date(form.submitted).toLocaleTimeString(\"en-US\", {\n                  timeStyle: \"short\",\n                })}\n            </Box>\n          </Box>\n        </Box>\n      </Box>\n\n      <Consultations title=\"Consultations\" consultations={form.consultations} />\n      <Consultations title=\"Procedures\" consultations={form.procedures} />\n\n      {form.images.length > 0 && (\n        <Box gap=\"medium\">\n          <Text\n            size=\"xlarge\"\n            weight=\"normal\"\n            margin=\"medium\"\n            alignSelf=\"center\"\n          >\n            Images\n          </Text>\n          <Box gap=\"large\" direction=\"row-responsive\">\n            {form.images &&\n              form.images.map((url) => (\n                <Box key={url} width=\"small\" border round overflow=\"hidden\">\n                  <Image src={url} />\n                </Box>\n              ))}\n          </Box>\n        </Box>\n      )}\n      {form.remarks && (\n        <Box>\n          <Text\n            size=\"xlarge\"\n            weight=\"normal\"\n            margin=\"medium\"\n            alignSelf=\"center\"\n          >\n            Notes\n          </Text>\n          <Paragraph textAlign=\"center\">{form.remarks}</Paragraph>\n        </Box>\n      )}\n\n      {form.cancelled ? (\n        <Button\n          disabled\n          label=\"Cancelled\"\n          color=\"status-critical\"\n          style={{ color: \"white\" }}\n        />\n      ) : (\n        <Button primary disabled={open} onClick={() => setOpen(true)}>\n          Cancel Appointment\n        </Button>\n      )}\n      {open && (\n        <Box gap=\"small\">\n          <Box direction=\"row\" align=\"start\" margin={{ vertical: \"medium\" }}>\n            <CheckBox\n              checked={checkBoxCancel}\n              onChange={(e) => setCheckBoxCancel(e.target.checked)}\n            />\n\n            <Text alignSelf=\"center\">\n              We ask that you give us at least 24 hours notice if you need to\n              cancel or reschedule an appointment.\n            </Text>\n          </Box>\n\n          <Box direction=\"row\" gap=\"medium\" justify=\"center\">\n            <Button\n              label=\"Cancel & Reschedule\"\n              onClick={() => {\n                setDisabled(true);\n                deleteAndReschedule(\n                  form._id,\n                  form.emailAddress,\n                  form.bookedFor,\n                  form.firstName + \" \" + form.lastName\n                );\n              }}\n              primary\n              disabled={!checkBoxCancel || disabled}\n            />\n            <Button\n              label=\"Just Cancel\"\n              primary\n              color=\"status-critical\"\n              onClick={() => {\n                setDisabled(true);\n\n                deleteForm(\n                  form._id,\n                  form.emailAddress,\n                  form.bookedFor,\n                  form.firstName + \" \" + form.lastName\n                );\n              }}\n              disabled={!checkBoxCancel || disabled}\n            />\n          </Box>\n        </Box>\n      )}\n    </Box>\n  );\n};\n\nexport default Appointment;\n","/Users/mahmedtan/Developer/derm-app/src/reducers/serviceTypeReducer.js",[],["501","502"],"/Users/mahmedtan/Developer/derm-app/src/services/procedures.js",[],["503","504"],"/Users/mahmedtan/Developer/derm-app/src/pages/Book/StepFooter.js",[],"/Users/mahmedtan/Developer/derm-app/src/components/Utils/BlockContentMain.js",[],["505","506"],"/Users/mahmedtan/Developer/derm-app/src/components/Sidebars/SidebarAccordian.js",["507"