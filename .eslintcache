\n    message: \"Enter a valid email address.\",\n    status: \"error\",\n  },\n];\nconst phoneNumberValidation = [\n  {\n    regexp: new RegExp(\"^\\\\(?([0-9]{3})\\\\) ?[-.●]?([0-9]{3})[-.●]?([0-9]{4})$\"),\n    message: \"Enter a valid phone number\",\n    status: \"error\",\n  },\n];\n\nconst phoneMask = [\n  { fixed: \"(\" },\n  {\n    length: 3,\n    regexp: /^[0-9]{1,3}$/,\n    placeholder: \"xxx\",\n  },\n  { fixed: \")\" },\n  { fixed: \" \" },\n  {\n    length: 3,\n    regexp: /^[0-9]{1,3}$/,\n    placeholder: \"xxx\",\n  },\n  { fixed: \"-\" },\n  {\n    length: 4,\n    regexp: /^[0-9]{1,4}$/,\n    placeholder: \"xxxx\",\n  },\n];\nconst StepTwo = () => {\n  return (\n    <Box animation=\"fadeIn\">\n      <Box direction=\"row-responsive\" gap=\"small\" width=\"medium\">\n        <FormField\n          required\n          label=\"First name\"\n          htmlFor=\"firstName-input\"\n          name=\"firstName\"\n        >\n          <TextInput placeholder=\"Jane\" id=\"firstName-input\" name=\"firstName\" />\n        </FormField>\n        <FormField\n          label=\"Last name\"\n          htmlFor=\"lastName-input\"\n          name=\"lastName\"\n          required\n        >\n          <TextInput placeholder=\"Smith\" id=\"lastName-input\" name=\"lastName\" />\n        </FormField>\n      </Box>\n      <FormField\n        label=\"Phone number\"\n        htmlFor=\"phoneNumber-input\"\n        name=\"phoneNumber\"\n        validate={phoneNumberValidation}\n        required\n      >\n        <MaskedInput\n          placeholder=\"(xxx) xxx-xxxx\"\n          type=\"tel\"\n          mask={phoneMask}\n          id=\"phoneNumber-input\"\n          name=\"phoneNumber\"\n        />\n      </FormField>\n      <FormField\n        required\n        label=\"Email address\"\n        htmlFor=\"emailAddress-input\"\n        name=\"emailAddress\"\n        validate={emailValidation}\n      >\n        <MaskedInput type=\"email\" id=\"emailAddress-input\" name=\"emailAddress\" />\n      </FormField>\n      <Box>\n        <FormField htmlFor=\"optionalCheckboxes\">\n          <CheckBoxGroup\n            id=\"optionalCheckboxes\"\n            name=\"offers\"\n            options={[\n              \"Would you like to receive text messages\",\n              \"Sign me up for offers\",\n            ]}\n          />\n        </FormField>\n      </Box>\n    </Box>\n  );\n};\n\nexport default StepTwo;\n",["449","450"],"/Users/mahmedtan/Developer/derm-app/src/pages/Book/Processing.js",["451"],"/Users/mahmedtan/Developer/derm-app/src/reducers/formValuesReducer.js",[],"/Users/mahmedtan/Developer/derm-app/src/reducers/dateReducer.js",[],"/Users/mahmedtan/Developer/derm-app/src/reducers/imageReducer.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Home.js",[],"/Users/mahmedtan/Developer/derm-app/src/components/StoryCards/StoryCard.js",[],["452","453"],"/Users/mahmedtan/Developer/derm-app/src/pages/Book/Confirmation.js",["454","455"],"import { Box, Heading, Card, Paragraph, Button } from \"grommet\";\nimport React, { useEffect, useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { Link } from \"react-router-dom\";\nimport Layout from \"../../components/Utils/Layout\";\nimport NotFound from \"../Extras/404\";\n\nconst Confirmation = () => {\n  const [fullName, setFullName] = useState(null);\n  const [submittedDate, setSubmittedDate] = useState(null);\n  const [error, setError] = useState(false);\n  useEffect(() => {\n    if (\n      window.sessionStorage.getItem(\"fullName\") &&\n      window.sessionStorage.getItem(\"submittedDate\")\n    ) {\n      setFullName(window.sessionStorage.getItem(\"fullName\"));\n      setSubmittedDate(window.sessionStorage.getItem(\"submittedDate\"));\n    } else setError(true);\n  }, []);\n  if (error)\n    return (\n      <Layout>\n        <NotFound />\n      </Layout>\n    );\n  return (\n    <Layout>\n      <Box\n        align=\"center\"\n        justify=\"start\"\n        height={{ min: \"100vh\" }}\n        margin=\"large\"\n      >\n        <Heading>Confirmation</Heading>\n        <Card align=\"center\">\n          <Box pad=\"medium\">\n            <Heading level=\"3\" margin=\"none\">\n              Dear {fullName},\n            </Heading>\n            <Paragraph size=\"large\">\n              Your appointment has been booked for {submittedDate}\n            </Paragraph>\n            <Link to=\"/profile\">\n              <Button primary label=\"Go to profile\" />\n            </Link>\n          </Box>\n        </Card>\n      </Box>\n    </Layout>\n  );\n};\n\nexport default Confirmation;\n",["456","457"],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Profile.js",["458","459"],"import React, { useEffect, useState } from \"react\";\nimport { useAuth0 } from \"@auth0/auth0-react\";\nimport { Box, Button, Heading } from \"grommet\";\nimport Layout from \"../../components/Utils/Layout\";\nimport VerifyBanner from \"./VerifyBanner\";\nimport { getForm, removeForm } from \"../../services/forms\";\nimport Appointments from \"./Appointments/Appointments\";\nimport Loading from \"../Extras/Loading\";\nimport PatientDetails from \"./Patient/PatientDetails\";\nimport { useHistory } from \"react-router-dom\";\n\nconst Profile = () => {\n  const { user, isAuthenticated } = useAuth0();\n  const history = useHistory();\n  const [fullName, setFullName] = useState(null);\n  const [forms, setForms] = useState(null);\n  useEffect(() => {\n    if (user) getForm(user.sub).then((res) => setForms(res));\n  }, [user]);\n  useEffect(() => {\n    if (forms)\n      setFullName(forms[0] && forms[0].firstName + \" \" + forms[0].lastName);\n    window.sessionStorage.clear();\n  }, [forms]);\n  useEffect(() => {\n    if (forms) {\n      const promises = forms\n        .filter((form) => new Date(form.bookedFor) < new Date())\n        .map((form) => removeForm(form._id));\n      Promise.all(promises).then((res) => {\n        console.log(res);\n        history.push(\"/profile\");\n      });\n    }\n  }, []);\n\n  if (!forms) return <Loading />;\n\n  return (\n    isAuthenticated && (\n      <Layout>\n        <Box\n          align=\"center\"\n          height={{ min: \"90vh\" }}\n          fill\n          gap=\"large\"\n          pad={{ bottom: \"large\" }}\n        >\n          <Heading\n            textAlign=\"center\"\n            level=\"2\"\n            margin={{ top: \"large\", bottom: \"none\" }}\n          >\n            Welcome {fullName}\n          </Heading>\n\n          {!user.email_verified && <VerifyBanner user={user} />}\n          <PatientDetails\n            setFullName={(fullName) => {\n              setFullName(fullName);\n            }}\n          />\n\n          <Appointments forms={forms} />\n        </Box>\n      </Layout>\n    )\n  );\n};\n\nexport default Profile;\n","/Users/mahmedtan/Developer/derm-app/src/services/forms.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Appointments/Consult.js",[],"/Users/mahmedtan/Developer/derm-app/src/auth/auth0-provider-with-history.js",[],"/Users/mahmedtan/Developer/derm-app/src/services/services.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Book/Steps/Steps.js",["460"],"import StepOne from \"./StepOne\";\nimport StepTwo from \"./StepTwo\";\nimport StepThree from \"./StepThree\";\nimport StepFour from \"./StepFour\";\n\nexport default [\n  {\n    title: \"Selection\",\n    description: \"Please select the item/items you want to talk about\",\n    item: <StepOne />,\n  },\n  {\n    title: \"Details\",\n    description: \"Please select the item/items you want to talk about\",\n    item: <StepTwo />,\n  },\n  {\n    title: \"Optional\",\n    description: \"Upload/Capture photos of your condition\",\n    item: <StepThree />,\n  },\n  {\n    title: \"Appointment\",\n    description: \"Please select your date and time of visit\",\n    item: <StepFour />,\n  },\n];\n","/Users/mahmedtan/Developer/derm-app/src/components/RecentBlogs/RecentBlogs.js",[],["461","462"],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Appointments/AppointmentCard.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Patient/PatientDetails.js",["463","464"],"import { useAuth0 } from \"@auth0/auth0-react\";\nimport { Box, Card, Heading, ResponsiveContext } from \"grommet\";\nimport React, { useContext, useEffect, useState } from \"react\";\nimport { getPatientDetails } from \"../../../services/patients\";\nimport Loading from \"../../Extras/Loading\";\nimport PatientNew from \"./PatientNew\";\nimport PatientOld from \"./PatientOld\";\n\nconst PatientDetails = ({ setFullName }) => {\n  const [details, setDetails] = useState(null);\n  const size = useContext(ResponsiveContext);\n  const [change, setChange] = useState(false);\n  const {\n    user: { sub },\n  } = useAuth0();\n\n  useEffect(() => {\n    getPatientDetails(sub).then((details) => {\n      !details.length ? setDetails({}) : setDetails(details[0]);\n    });\n  }, [change]);\n\n  useEffect(() => {\n    details && details.fullName && setFullName(details.fullName);\n  }, [details]);\n  if (!details) return <Loading />;\n\n  return (\n    <Box align=\"center\" pad={{ horizontal: \"large\" }}>\n      <Heading\n        level=\"3\"\n        textAlign=\"center\"\n        margin={{ bottom: \"large\", top: \"none\" }}\n        color=\"text\"\n      >\n        Profile\n      </Heading>\n      <Card\n        pad={{\n          vertical: size === \"small\" ? \"medium\" : \"large\",\n          horizontal: \"large\",\n        }}\n        animation=\"zoomOut\"\n      >\n        {Object.keys(details).length === 0 && details.constructor === Object ? (\n          <PatientNew\n            sub={sub}\n            setChange={() => {\n              setChange(!change);\n            }}\n          />\n        ) : (\n          <PatientOld\n            sub={sub}\n            details={details}\n            setChange={() => {\n              setChange(!change);\n            }}\n          />\n        )}\n      </Card>\n    </Box>\n  );\n};\n\nexport default PatientDetails;\n","/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Patient/PatientNew.js",["465","466"],"import { Box, Paragraph, Text } from \"grommet\";\nimport React, { useState } from \"react\";\nimport EditProfile from \"./EditProfile\";\n\nconst PatientNew = ({ sub, setChange }) => {\n  const [message, setMessage] = useState(null);\n  return (\n    <Box align=\"center\" width=\"medium\">\n      <Paragraph>You have no previous Information stored </Paragraph>\n      <Box width=\"medium\">\n        <EditProfile\n          setChange={setChange}\n          sub={sub}\n          setMessage={(message) => {\n            setMessage(message);\n          }}\n        />\n      </Box>\n    </Box>\n  );\n};\n\nexport default PatientNew;\n",["467","468"],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Patient/EditProfile.js",["469"],"import React, { useContext, useState } from \"react\";\nimport {\n  Box,\n  Button,\n  Form,\n  FormField,\n  Header,\n  MaskedInput,\n  Text,\n  TextInput,\n  ResponsiveContext,\n} from \"grommet\";\nimport { updatePatientDetails } from \"../../../services/patients\";\nimport { v4 as uuidv4 } from \"uuid\";\n\nconst emailValidation = [\n  {\n    regexp: new RegExp(\"[^@ \\\\t\\\\r\\\\n]+@\"),\n    message: \"Enter a valid email address.\",\n    status: \"error\",\n  },\n  {\n    regexp: new RegExp(\"[^@ \\\\t\\\\r\\\\n]+@[^@ \\\\t\\\\r\\\\n]+\\\\.[^@ \\\\t\\\\r\\\\n]+\"),\n    message: \"Enter a valid email address.\",\n    status: \"error\",\n  },\n  {\n    regexp: new RegExp(\"[^@ \\\\t\\\\r\\\\n]+@[^@ \\\\t\\\\r\\\\n]+\\\\.[^@ \\\\t\\\\r\\\\n]+\"),\n    message: \"Enter a valid email address.\",\n    status: \"error\",\n  },\n];\n\nconst phoneNumberValidation = [\n  {\n    regexp: new RegExp(\"^\\\\(?([0-9]{3})\\\\) ?[-.●]?([0-9]{3})[-.●]?([0-9]{4})$\"),\n    message: \"Enter a valid phone number\",\n    status: \"error\",\n  },\n];\n\nconst emailMask = [\n  {\n    regexp: /^[\\w\\-_.]+$/,\n    placeholder: \"jane.smith\",\n  },\n  { fixed: \"@\" },\n  {\n    regexp: /^[\\w]+$/,\n    placeholder: \"hpe\",\n  },\n  { fixed: \".\" },\n  {\n    regexp: /^[\\w]+$/,\n    placeholder: \"com\",\n  },\n];\nconst phoneMask = [\n  { fixed: \"(\" },\n  {\n    length: 3,\n    regexp: /^[0-9]{1,3}$/,\n    placeholder: \"xxx\",\n  },\n  { fixed: \")\" },\n  { fixed: \" \" },\n  {\n    length: 3,\n    regexp: /^[0-9]{1,3}$/,\n    placeholder: \"xxx\",\n  },\n  { fixed: \"-\" },\n  {\n    length: 4,\n    regexp: /^[0-9]{1,4}$/,\n    placeholder: \"xxxx\",\n  },\n];\n\nexport const EditProfile = ({\n  emailAddress,\n  fullName,\n  phoneNumber,\n  setChange,\n  _id,\n  setMessage,\n  patientId,\n  sub,\n}) => {\n  const [formValues, setFormValues] = React.useState({\n    emailAddress: emailAddress || \"\",\n    fullName: fullName || \"\",\n    phoneNumber: phoneNumber || \"\",\n  });\n\n  const [open, setOpen] = useState(false);\n  const [disabled, setDisabled] = useState(true);\n\n  const preliminaryValidation = !(\n    formValues.fullName.length >= 3 &&\n    new RegExp(\"^\\\\(?([0-9]{3})\\\\) ?[-.●]?([0-9]{3})[-.●]?([0-9]{4})$\").test(\n      formValues.phoneNumber\n    ) &&\n    new RegExp(\"[^@ \\\\t\\\\r\\\\n]+@[^@ \\\\t\\\\r\\\\n]+\\\\.[^@ \\\\t\\\\r\\\\n]+\").test(\n      formValues.emailAddress\n    )\n  );\n  const size = useContext(ResponsiveContext);\n\n  const onChange = (values) => {\n    setDisabled(false);\n    setFormValues(values);\n  };\n\n  // eslint-disable-next-line no-unused-vars\n  const onSubmit = ({ value, touched }) => {\n    setOpen(false);\n    updatePatientDetails({\n      ...value,\n      fullName: fullName || value.fullName,\n      sub,\n      id: _id || uuidv4(),\n      patientId: patientId || uuidv4().substring(0, 8),\n    }).then((res, error) => {\n      setMessage(\"Your Profile has been updated\");\n      console.log(res);\n      setChange();\n    });\n  };\n  if (!open)\n    return (\n      <Button\n        label=\"Edit Profile\"\n        primary\n        onClick={() => {\n          setOpen(true);\n        }}\n      />\n    );\n  return (\n    <Box gap=\"medium\" align=\"center\" animation=\"slideDown\">\n      <Text size=\"xlarge\" weight=\"normal\" margin=\"small\">\n        Edit Profile\n      </Text>\n\n      <Box pad={{ horizontal: \"xxsmall\" }} fill>\n        <Form\n          validate=\"blur\"\n          value={formValues}\n          onChange={(nextValue) => onChange(nextValue)}\n          onSubmit={({ value, touched }) => onSubmit({ value, touched })}\n        >\n          {!fullName && (\n            <FormField\n              label=\"Full Name\"\n              htmlFor=\"fullName-input-persona\"\n              name=\"fullName\"\n              validate={(value) =>\n                value.length < 3 && \"Name must at least contain 3 letters \"\n              }\n              required\n            >\n              <TextInput\n                id=\"fullName-input-personal\"\n                name=\"fullName\"\n                placeholder=\"Jane Smith\"\n              />\n            </FormField>\n          )}\n\n          <FormField\n            label=\"Phone number\"\n            htmlFor=\"phoneNumber-input-personal\"\n            name=\"phoneNumber\"\n            validate={phoneNumberValidation}\n            required\n          >\n            <MaskedInput\n              placeholder=\"(xxx) xxx-xxxx\"\n              type=\"tel\"\n              mask={phoneMask}\n              id=\"phoneNumber-input-personal\"\n              name=\"phoneNumber\"\n            />\n          </FormField>\n          <FormField\n            required\n            label=\"Email address\"\n            htmlFor=\"emailAddress-input-personal\"\n            name=\"emailAddress\"\n            validate={emailValidation}\n          >\n            <MaskedInput\n              type=\"email\"\n              mask={emailMask}\n              id=\"emailAddress-input-personal\"\n              name=\"emailAddress\"\n            />\n          </FormField>\n\n          <Box\n            align={size !== \"small\" ? \"start\" : undefined}\n            margin={{ top: \"medium\", bottom: \"small\" }}\n            direction=\"row\"\n            gap=\"small\"\n          >\n            <Button label=\"Cancel\" secondary onClick={() => setOpen(false)} />\n            <Button\n              label=\"Confirm\"\n              primary\n              disabled={preliminaryValidation || disabled}\n              type=\"submit\"\n            />\n          </Box>\n        </Form>\n      </Box>\n    </Box>\n  );\n};\n\nexport default EditProfile;\n","/Users/mahmedtan/Developer/derm-app/src/reducers/selectedItemReducer.js",[],"/Users/mahmedtan/Developer/derm-app/src/services/consultations.js",[],["470","471"],"/Users/mahmedtan/Developer/derm-app/src/pages/Services/Service.js",["472"],"/Users/mahmedtan/Developer/derm-app/src/services/patients.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Patient/PatientOld.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Patient/DetailsField.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/VerifyBanner.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Appointments/Appointments.js",["473","474","475"],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Appointments/Appointment.js",["476"],"import { Card, Box, Text, Image, Button, Paragraph } from \"grommet\";\nimport React, { useState } from \"react\";\nimport Consultations from \"./Consult\";\n\nconst Appointment = ({ form, deleteForm, deleteAndReschedule }) => {\n  const [open, setOpen] = useState(false);\n\n  return (\n    <Box\n      align=\"center\"\n      gap=\"medium\"\n      margin=\"medium\"\n      animation=\"fadeIn\"\n      width=\"large\"\n    >\n      <Text size=\"xlarge\" weight=\"normal\" margin=\"medium\" alignSelf=\"center\">\n        Personal Information\n      </Text>\n      <Box\n        pad={{ horizontal: \"xlarge\" }}\n        direction=\"row-responsive\"\n        justify=\"between\"\n        gap=\"large\"\n        fill\n        align=\"center\"\n      >\n        <Box gap=\"large\">\n          <Box>\n            <Text weight=\"bold\" size=\"large\">\n              First Name\n            </Text>\n            {form.firstName}\n          </Box>\n          <Box>\n            <Text weight=\"bold\" size=\"large\">\n              Email Address\n            </Text>\n            {form.emailAddress}\n          </Box>\n          <Box>\n            <Text weight=\"bold\" size=\"large\">\n              Booked For\n            </Text>\n            {new Date(form.bookedFor).toLocaleDateString(\"en-US\", {\n              year: \"numeric\",\n              month: \"long\",\n              day: \"numeric\",\n            }) +\n              \" \" +\n              new Date(form.bookedFor).toLocaleTimeString(\"en-US\", {\n                timeStyle: \"short\",\n              })}\n          </Box>\n        </Box>\n        <Box gap=\"large\">\n          <Box>\n            <Text weight=\"bold\" size=\"large\">\n              Last Name\n            </Text>\n            {form.lastName}\n          </Box>\n          <Box>\n            <Text weight=\"bold\" size=\"large\">\n              Phone Number\n            </Text>\n            {form.phoneNumber}\n          </Box>\n          <Box>\n            <Text weight=\"bold\" size=\"large\">\n              Submitted at\n            </Text>\n            {new Date(form.submitted).toLocaleDateString(\"en-US\", {\n              year: \"numeric\",\n              month: \"long\",\n              day: \"numeric\",\n            }) +\n              \" \" +\n              new Date(form.submitted).toLocaleTimeString(\"en-US\", {\n                timeStyle: \"short\",\n              })}\n          </Box>\n        </Box>\n      </Box>\n      <Consultations title=\"Consultations\" consultations={form.consultations} />\n      <Consultations title=\"Procedures\" consultations={form.procedures} />\n\n      {form.images.length > 0 && (\n        <Box gap=\"medium\">\n          <Text\n            size=\"xlarge\"\n            weight=\"normal\"\n            margin=\"medium\"\n            alignSelf=\"center\"\n          >\n            Images\n          </Text>\n          <Box gap=\"large\" direction=\"row-responsive\">\n            {form.images &&\n              form.images.map((url) => (\n                <Box key={url} width=\"small\" border round overflow=\"hidden\">\n                  <Image src={url} />\n                </Box>\n              ))}\n          </Box>\n        </Box>\n      )}\n      {form.remarks && (\n        <Box>\n          <Text\n            size=\"xlarge\"\n            weight=\"normal\"\n            margin=\"medium\"\n            alignSelf=\"center\"\n          >\n            Notes\n          </Text>\n          <Paragraph>{form.remarks}</Paragraph>\n        </Box>\n      )}\n\n      {form.cancelled ? (\n        <Button secondary disabled label=\"cancelled\" />\n      ) : (\n        <Button primary disabled={open} onClick={() => setOpen(true)}>\n          Cancel Appointment\n        </Button>\n      )}\n      {open && (\n        <Box gap=\"small\">\n          <Text textAlign=\"center\">\n            Are you sure about this!! Lorem ipsum dolor sit amet, consectetur\n            adipisicing elit. Eveniet, culpa.\n          </Text>\n          <Box direction=\"row\" gap=\"medium\" justify=\"center\">\n            <Button\n              label=\"Cancel & Reschedule\"\n              onClick={() => deleteAndReschedule(form._id)}\n              primary\n            />\n            <Button\n              label=\"Just Cancel\"\n              primary\n              color=\"status-critical\"\n              onClick={() => deleteForm(form._id)}\n            />\n          </Box>\n        </Box>\n      )}\n    </Box>\n  );\n};\n\nexport default Appointment;\n","/Users/mahmedtan/Developer/derm-app/src/reducers/serviceTypeReducer.js",[],["477","478"],"/Users/ma