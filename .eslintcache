,"import React, { useContext } from \"react\";\nimport { FormPrevious } from \"grommet-icons\";\nimport { Link } from \"react-router-dom\";\nimport Steps from \"./Steps/Steps\";\n\nimport { Box, Heading, Button, Text, ResponsiveContext } from \"grommet\";\n\nimport { decrementIndex } from \"../../reducers/indexReducer\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\nconst StepHeader = () => {\n  const dispatch = useDispatch();\n  const size = useContext(ResponsiveContext);\n  const activeIndex = useSelector(({ activeIndex }) => activeIndex);\n\n  return (\n    <Box gap=\"large\" align=\"start\" justify=\"center\">\n      <Box fill align=\"center\">\n        {size === \"small\" &&\n          (activeIndex !== 0 ? (\n            <Button\n              label=\"Back\"\n              onClick={() => dispatch(decrementIndex())}\n              style={{ boxShadow: \"2px 2px 10px -5px black\" }}\n            />\n          ) : (\n            <Link to=\"/services\">\n              <Button          style={{ boxShadow: \"2px 2px 10px -5px black\" }}\n size=\"small\" label=\"Back to Services\" />\n            </Link>\n          ))}\n      </Box>\n\n      <Box\n        direction=\"row\"\n        fill=\"horizontal\"\n        align=\"center\"\n        justify={size === \"small\" ? \"center\" : \"between\"}\n      >\n        {size !== \"small\" &&\n          (activeIndex !== 0 ? (\n            <Button\n              icon={<FormPrevious />}\n              label=\"Back\"\n              onClick={() => dispatch(decrementIndex())}\n            />\n          ) : (\n            <Link to=\"/services\">\n              <Button\n                icon={<FormPrevious />}\n                size=\"small\"\n                label=\"Back to Services\"\n              />\n            </Link>\n          ))}\n        <Text textAlign=\"center\" weight=\"normal\">{`Step ${activeIndex + 1} of ${\n          Steps.length\n        }`}</Text>\n      </Box>\n    </Box>\n  );\n};\n\nexport default StepHeader;\n",["457","458"],"/Users/mahmedtan/Developer/derm-app/src/components/Slider/Slider.js",["459","460"],"import { ResponsiveContext, Box } from \"grommet\";\nimport { LinkNext, LinkPrevious } from \"grommet-icons\";\nimport { useContext, useEffect, useState } from \"react\";\nimport { useSelector } from \"react-redux\";\nimport Slider from \"react-slick\";\nimport Loading from \"../../pages/Extras/Loading\";\n\nimport SliderImage from \"./SliderImage\";\n\nconst ResponsiveSlider = () => {\n  let services = useSelector(({ services }) => services);\n  const size = useContext(ResponsiveContext);\n\n  if (!services) return <Loading />;\n\n  services = services.filter((service) => service.showOnSlider);\n\n  const getNumOfItemsToShow = (size) => {\n    return size === \"small\" ? 1 : 3;\n  };\n  const PrevArrow = (props) => (\n    <LinkPrevious className={props.className} onClick={props.onClick} />\n  );\n  const NextArrow = (props) => (\n    <LinkNext className={props.className} onClick={props.onClick} />\n  );\n\n  const settings = {\n    slidesToScroll: size === \"small\" ? 1 : 3,\n    speed: 2500,\n    autoplaySpeed: 5000,\n    swipe: true,\n    swipeToScroll: true,\n    pauseOnHover: true,\n    arrows: size !== \"small\",\n    autoplay: true,\n    dots: true,\n    prevArrow: <PrevArrow />,\n    nextArrow: <NextArrow />,\n  };\n\n  return (\n    <Box width=\"xlarge\" pad={{ horizontal: \"medium\" }}>\n      <Slider slidesToShow={getNumOfItemsToShow(size)} {...settings}>\n        {services\n          .sort((a, b) => a.order - b.order)\n          .map((service) => (\n            <SliderImage key={service.slug} {...service} size={size} />\n          ))}\n      </Slider>\n    </Box>\n  );\n};\n\nexport default ResponsiveSlider;\n",["461","462"],"/Users/mahmedtan/Developer/derm-app/src/components/RecentBlogs/RecentBlogCard.js",["463","464","465"],"import React from \"react\";\nimport { Heading, Text, Box, Image, Avatar } from \"grommet\";\nimport Linker from \"../Utils/Linker\";\n\nconst RecentBlogCard = (blog) => {\n  return (\n    <Box width=\"medium\" pad=\"medium\">\n      <Linker to={`/blogs/${blog.slug}`}>\n        <Box elevation=\"medium\" height=\"15rem\" round=\"small\" overflow=\"hidden\">\n          <Image fit=\"cover\" src={blog.mainImage} a11yTitle=\"bridge\" />\n        </Box>\n      </Linker>\n\n      <Box>\n        <Linker to={`/blogs/${blog.slug}`}>\n          <Heading level={3} color=\"text\" textAlign=\"center\" color=\"#131600\">\n            {blog.title}\n          </Heading>\n        </Linker>\n      </Box>\n    </Box>\n  );\n};\n\nexport default RecentBlogCard;\n","/Users/mahmedtan/Developer/derm-app/src/services/slots.js",[],["466","467"],"/Users/mahmedtan/Developer/derm-app/src/components/Sidebars/SidebarServices.js",[],["468","469"],"/Users/mahmedtan/Developer/derm-app/src/components/Authentication/AuthenticationSnackBar.js",["470"],"/Users/mahmedtan/Developer/derm-app/src/components/Authentication/AuthenticationSnackbar.js",["471"],"import { useAuth0 } from \"@auth0/auth0-react\";\nimport { Box, Button } from \"grommet\";\nimport React from \"react\";\nimport LoginBtn from \"./LoginBtn\";\nimport { Link } from \"react-router-dom\";\nimport { User } from \"grommet-icons\";\n\nconst AutheticationSnackbar = () => {\n  const { isAuthenticated } = useAuth0();\n\n  return isAuthenticated ? (\n    <Link to=\"/profile\">\n      <Button icon={<User />} />\n    </Link>\n  ) : (\n    <LoginBtn primary />\n  );\n};\n\nexport default AutheticationSnackbar;\n","/Users/mahmedtan/Developer/derm-app/src/reducers/uiThemeReducer.js",[],"/Users/mahmedtan/Developer/derm-app/src/components/App/App.js",[],"/Users/mahmedtan/Developer/derm-app/src/auth/protected-route.js",[],"/Users/mahmedtan/Developer/derm-app/src/components/Comments/Comments.js",[],["472","473"],"/Users/mahmedtan/Developer/derm-app/src/components/Comments/Comment.js",["474"],"import React from \"react\";\nimport { Box, Heading, Paragraph, Text } from \"grommet\";\n\nconst Comment = ({ patientName, remarks }) => {\n  return (\n    <Box align=\"center\">\n      <Paragraph weight=\"bold\" size=\"xlarge\" textAlign=\"center\">\n        <Text size=\"xxlarge\">\" </Text>\n        {remarks}\n        <Text size=\"xxlarge\"> \"</Text>\n      </Paragraph>\n      <Text weight=\"bold\" size=\"xlarge\">\n        {patientName}\n      </Text>\n    </Box>\n  );\n};\n\nexport default Comment;\n",["475","476"],"/Users/mahmedtan/Developer/derm-app/src/components/Utils/OfferCard.js",["477"],"/Users/mahmedtan/Developer/derm-app/src/pages/Book/Book.js",["478","479"],"import {\n  Box,\n  Heading,\n  Form,\n  Text,\n  ResponsiveContext,\n  Paragraph,\n} from \"grommet\";\nimport React, { useContext, useEffect, useRef } from \"react\";\n\nimport StepFooter from \"./StepFooter\";\nimport { useHistory } from \"react-router-dom\";\nimport Steps from \"./Steps/Steps\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport StepHeader from \"./StepHeader\";\n\nimport Loading from \"../Extras/Loading\";\nimport { changeValues } from \"../../reducers/formValuesReducer\";\nimport { uploadImages } from \"../../services/forms\";\nimport { useState } from \"react\";\nimport Spinner from \"../../components/Utils/Spinner\";\n\nconst Book = () => {\n  const size = useContext(ResponsiveContext);\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const [loading, setLoading] = useState(false);\n\n  const {\n    activeIndex,\n    procedures,\n    consultations,\n    formValues,\n    images,\n  } = useSelector(\n    ({ procedures, consultations, activeIndex, formValues, images }) => ({\n      activeIndex,\n      formValues,\n      procedures,\n      consultations,\n      images,\n    })\n  );\n\n  if (!(procedures && consultations && images)) {\n    return <Loading />;\n  }\n  if (loading)\n    return (\n      <Box\n        align=\"center\"\n        justify=\"center\"\n        height={{ min: \"100vh\" }}\n        pad=\"large\"\n      >\n        <Heading textAlign=\"center\">Processing your application</Heading>\n        <Box height=\"medium\">\n          <Spinner />\n        </Box>\n        <Paragraph>\n          This might take a minute or two depending on the number of images\n          you've uploaded. Meanwhile, Please do not refresh or close this page.\n        </Paragraph>\n      </Box>\n    );\n\n  return (\n    <Box align=\"center\" height={{ min: \"100vh\" }} margin={{ bottom: \"xlarge\" }}>\n      <Box\n        align=\"center\"\n        width=\"large\"\n        pad={size === \"small\" ? \"large\" : \"medium\"}\n        gap=\"medium\"\n      >\n        <Box gap=\"small\" fill>\n          <StepHeader />\n          <Heading margin={{ vertical: \"none\" }} level=\"2\" textAlign=\"center\">\n            {Steps[activeIndex].title}\n          </Heading>\n          <Text textAlign=\"center\">{Steps[activeIndex].description}</Text>\n        </Box>\n\n        <Box>\n          <Form\n            validate=\"blur\"\n            value={formValues}\n            onChange={(nextValue) => dispatch(changeValues(nextValue))}\n            onSubmit={({ value }) => {\n              setLoading(true);\n              images\n                ? uploadImages(images).then((res) => {\n                    window.sessionStorage.setItem(\n                      \"images\",\n                      JSON.stringify(res)\n                    );\n                    history.push(\"/processing\");\n                  })\n                : history.push(\"/processing\");\n            }}\n          >\n            <Box\n              gap=\"small\"\n              margin={{\n                bottom: \"xlarge\",\n                top: size === \"small\" ? \"large\" : \"none\",\n              }}\n            >\n              {Steps[activeIndex].item}\n              <StepFooter />\n            </Box>\n          </Form>\n        </Box>\n      </Box>\n    </Box>\n  );\n};\n\nexport default Book;\n","/Users/mahmedtan/Developer/derm-app/src/components/Header/Header.js",[],["480","481"],"/Users/mahmedtan/Developer/derm-app/src/pages/Book/Processing.js",["482","483"],"import { useAuth0 } from \"@auth0/auth0-react\";\nimport Axios from \"axios\";\nimport React, { useEffect, useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { useHistory } from \"react-router-dom\";\nimport Layout from \"../../components/Utils/Layout\";\nimport { submitForm } from \"../../services/forms\";\nimport NotFound from \"../Extras/404\";\nimport Loading from \"../Extras/Loading\";\n\nconst Processing = () => {\n  const [submitted, setSubmitted] = useState(false);\n\n  const [error, setError] = useState(false);\n\n  const dispatch = useDispatch();\n\n  const history = useHistory();\n\n  const { formValues, procedures, consultations, date } = useSelector(\n    ({ formValues, procedures, consultations, date }) => ({\n      formValues,\n      procedures,\n      consultations,\n      date,\n    })\n  );\n  const { user } = useAuth0();\n\n  useEffect(() => {\n    if (!submitted) {\n      if (formValues && procedures && consultations && date && user) {\n        const images =\n          window.sessionStorage.getItem(\"images\") &&\n          JSON.parse(window.sessionStorage.getItem(\"images\"));\n\n        submitForm(formValues, images, procedures, consultations, date, user)\n          .then((response) => {\n            console.log(\"Form Submitted\", response);\n\n            Axios.post(\"/api/mail\", {\n              formValues,\n              date,\n              procedures,\n              consultations,\n            }).then((res) => {\n              console.log(\"Mail sent\", res);\n\n              setSubmitted(true);\n            });\n          })\n          .catch((err) => {\n            console.log(err);\n\n            dispatch({ type: \"RESET_FORM\" });\n            dispatch({ type: \"RESET_INDEX\" });\n            dispatch({ type: \"RESET_IMAGES\" });\n            dispatch({ type: \"RESET_DATE\" });\n            setError(\"Sorry for inconvenience\");\n          });\n      }\n      if (formValues.emailAddress === \"\")\n        setError(\"Looks like you're on the wrong page\");\n    }\n  }, [formValues, procedures, consultations, date, dispatch, submitted]);\n  useEffect(() => {\n    if (submitted) {\n      window.sessionStorage.clear();\n      window.sessionStorage.setItem(\n        \"fullName\",\n        formValues.firstName + \" \" + formValues.lastName\n      );\n      window.sessionStorage.setItem(\"submittedDate\", date);\n\n      dispatch({ type: \"RESET_FORM\" });\n      dispatch({ type: \"RESET_INDEX\" });\n      dispatch({ type: \"RESET_IMAGES\" });\n      dispatch({ type: \"RESET_DATE\" });\n\n      history.push(\"/confirmation\");\n    }\n  }, [submitted, dispatch]);\n\n  if (error)\n    return (\n      <Layout>\n        <NotFound />\n      </Layout>\n    );\n  if (!submitted) return <Loading />;\n  return \"Yes\";\n};\n\nexport default Processing;\n","/Users/mahmedtan/Developer/derm-app/src/reducers/formValuesReducer.js",[],"/Users/mahmedtan/Developer/derm-app/src/reducers/dateReducer.js",[],["484","485"],"/Users/mahmedtan/Developer/derm-app/src/reducers/imageReducer.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Home.js",[],"/Users/mahmedtan/Developer/derm-app/src/components/StoryCards/StoryCard.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Book/Confirmation.js",["486"],"import { Box, Heading, Card, Paragraph, Button, Text } from \"grommet\";\nimport React, { useEffect, useState } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport Layout from \"../../components/Utils/Layout\";\nimport NotFound from \"../Extras/404\";\nimport Loading from \"../Extras/Loading\";\n\nconst Confirmation = () => {\n  const [fullName, setFullName] = useState(null);\n  const [submittedDate, setSubmittedDate] = useState(null);\n  const [error, setError] = useState(false);\n\n  useEffect(() => {\n    if (\n      window.sessionStorage.getItem(\"fullName\") &&\n      window.sessionStorage.getItem(\"submittedDate\")\n    ) {\n      setFullName(window.sessionStorage.getItem(\"fullName\"));\n      setSubmittedDate(window.sessionStorage.getItem(\"submittedDate\"));\n    } else setError(true);\n  }, []);\n  if (error)\n    return (\n      <Layout>\n        <NotFound />\n      </Layout>\n    );\n\n  return (\n    <Layout>\n      <Box\n        align=\"center\"\n        justify=\"start\"\n        height={{ min: \"100vh\" }}\n        margin=\"large\"\n      >\n        <Heading>You are all set!</Heading>\n        <Card align=\"center\">\n          <Box pad=\"medium\" align=\"center\" gap=\"small\">\n            <Text size=\"xlarge\">Dear {fullName}</Text>\n            <Paragraph size=\"large\" textAlign=\"center\">\n              Your appointment has been booked for{\" \"}\n              {new Date(submittedDate).toLocaleDateString(\"en-US\", {\n                year: \"numeric\",\n                month: \"long\",\n                day: \"numeric\",\n              }) +\n                \" at \" +\n                new Date(submittedDate).toLocaleTimeString(\"en-US\", {\n                  timeStyle: \"short\",\n                })}\n            </Paragraph>\n            <Link to=\"/profile\">\n              <Button primary label=\"Go to profile\" />\n            </Link>\n          </Box>\n        </Card>\n      </Box>\n    </Layout>\n  );\n};\n\nexport default Confirmation;\n","/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Profile.js",["487","488"],"/Users/mahmedtan/Developer/derm-app/src/services/forms.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Appointments/Consult.js",[],"/Users/mahmedtan/Developer/derm-app/src/auth/auth0-provider-with-history.js",[],["489","490"],"/Users/mahmedtan/Developer/derm-app/src/services/services.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Book/Steps/Steps.js",["491"],"/Users/mahmedtan/Developer/derm-app/src/components/RecentBlogs/RecentBlog