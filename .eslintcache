tations,\n            }).then((res) => {\n              console.log(\"Mail sent\", res);\n\n              setSubmitted(true);\n            });\n          })\n          .catch((err) => {\n            console.log(err);\n\n            dispatch({ type: \"RESET_FORM\" });\n            dispatch({ type: \"RESET_INDEX\" });\n            dispatch({ type: \"RESET_IMAGES\" });\n            dispatch({ type: \"RESET_DATE\" });\n            setError(\"Sorry for inconvenience\");\n          });\n      }\n      if (formValues.emailAddress === \"\")\n        setError(\"Looks like you're on the wrong page\");\n    }\n  }, [formValues, procedures, consultations, date, dispatch, submitted]);\n  useEffect(() => {\n    if (submitted) {\n      window.sessionStorage.clear();\n      window.sessionStorage.setItem(\n        \"fullName\",\n        formValues.firstName + \" \" + formValues.lastName\n      );\n      window.sessionStorage.setItem(\"submittedDate\", date);\n\n      dispatch({ type: \"RESET_FORM\" });\n      dispatch({ type: \"RESET_INDEX\" });\n      dispatch({ type: \"RESET_IMAGES\" });\n      dispatch({ type: \"RESET_DATE\" });\n\n      history.push(\"/confirmation\");\n    }\n  }, [submitted, dispatch]);\n\n  if (error)\n    return (\n      <Layout>\n        <NotFound />\n      </Layout>\n    );\n  if (!submitted) return <Loading />;\n  return \"Yes\";\n};\n\nexport default Processing;\n","/Users/mahmedtan/Developer/derm-app/src/reducers/formValuesReducer.js",[],"/Users/mahmedtan/Developer/derm-app/src/reducers/dateReducer.js",[],["480","481"],"/Users/mahmedtan/Developer/derm-app/src/reducers/imageReducer.js",[],["482","483"],"/Users/mahmedtan/Developer/derm-app/src/pages/Home.js",[],"/Users/mahmedtan/Developer/derm-app/src/components/StoryCards/StoryCard.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Book/Confirmation.js",["484"],"import { Box, Heading, Card, Paragraph, Button } from \"grommet\";\nimport React, { useEffect, useState } from \"react\";\nimport { Link } fr