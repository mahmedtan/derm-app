patch(initProcedures());\n\n    const formValues = window.sessionStorage.getItem(\"formValues\");\n    const date = window.sessionStorage.getItem(\"date\");\n    if (formValues && date) {\n      dispatch(changeValues(JSON.parse(formValues)));\n      dispatch(changeDate(new Date(date)));\n    }\n    if (window.sessionStorage.getItem(\"uiTheme\")) {\n      dispatch({\n        type: \"SET_THEME\",\n        uiTheme: window.localStorage.getItem(\"uiTheme\"),\n      });\n    }\n  }, [dispatch]);\n\n  if (isLoading) return <Loading />;\n\n  return (\n    <Grommet theme={theme} themeMode={uiTheme} full>\n      <Switch>\n        <Route path=\"/blogs/:slug\">\n          <Blog />\n        </Route>\n\n        <Route path=\"/blogs\">\n          <Blogs blogs={blogs} />\n        </Route>\n\n        <ProtectedRoute path=\"/profile\" component={Profile} />\n\n        <ProtectedRoute path=\"/processing\" component={Processing} />\n\n        <ProtectedRoute path=\"/confirmation\" component={Confirmation} />\n\n        <Route path=\"/book-now\" component={Book} />\n\n        <Route path=\"/services/:slug\">\n          <Services />\n        </Route>\n        <Route path=\"/services\">\n          <Layout>\n            <ServicesMain />\n          </Layout>\n        </Route>\n        <Route path=\"/finance\">\n          <Layout>\n            <Finance />\n          </Layout>\n        </Route>\n        <Route path=\"/about-us\">\n          <Layout>\n            <AboutUs />\n          </Layout>\n        </Route>\n        <Route path=\"/contact-us\">\n          <Layout>\n            <ContactUs />\n          </Layout>\n        </Route>\n        <Route path=\"/specials\">\n          <Layout>\n            <Specials />\n          </Layout>\n        </Route>\n        <Route exact path=\"/\">\n          <Layout>\n            <Home />\n          </Layout>\n        </Route>\n        <Route path=\"*\">\n          <Layout>\n            <NotFound />\n          </Layout>\n        </Route>\n      </Switch>\n    </Grommet>\n  );\n}\n\nexport default App;\n",["465","466"],"/Users/mahmedtan/Developer/derm-app/src/auth/protected-route.js",[],["467","468"],"/Users/mahmedtan/Developer/derm-app/src/components/Comments/Comments.js",[],"/Users/mahmedtan/Developer/derm-app/src/components/Comments/Comment.js",["469"],"import React from \"react\";\nimport { Box, Heading, Paragraph, Text } from \"grommet\";\n\nconst Comment = ({ patientName, remarks }) => {\n  return (\n    <Box align=\"center\">\n      <Paragraph weight=\"bold\" size=\"xlarge\" textAlign=\"center\">\n        <Text size=\"xxlarge\">\" </Text>\n        {remarks}\n        <Text size=\"xxlarge\"> \"</Text>\n      </Paragraph>\n      <Text weight=\"bold\" size=\"xlarge\">\n        {patientName}\n      </Text>\n    </Box>\n  );\n};\n\nexport default Comment;\n","/Users/mahmedtan/Developer/derm-app/src/components/Utils/OfferCard.js",["470"],"import {\n  Card,\n  Box,\n  Text,\n  Paragraph,\n  ResponsiveContext,\n  CheckBox,\n} from \"grommet\";\nimport React, { useContext, useEffect, useState } from \"react\";\nimport { useSelector } from \"react-redux\";\n\nconst OfferCard = ({ title, price, time, description, _id, type, item }) => {\n  const formValues = useSelector(({ formValues }) => formValues);\n  const [selected, setSelected] = useState(false);\n  const size = useContext(ResponsiveContext);\n  useEffect(() => {\n    setSelected(\n      Object.keys(formValues)\n        .filter((x) => formValues[x])\n        .indexOf(_id) >= 0\n    );\n  }, [formValues]);\n\n  return (\n    <Card\n      pad={size === \"small\" ? \"large\" : \"medium\"}\n      justify=\"center\"\n      border={selected && { color: \"#8E677A\", size: \"small\" }}\n      elevation={selected ? \"large\" : \"small\"}\n      round={size === \"small\" ? \"medium\" : \"small\"}\n    >\n      <Box direction=\"row\" justify=\"between\">\n        <Text size=\"large\">{title}</Text>\n        <Text>\n          <em>{`${time} min`}</em>\n        </Text>\n      </Box>\n\n      <Box height=\"xsmall\" overflow=\"hidden\">\n        <Paragraph>{description}</Paragraph>\n      </Box>\n\n      <Box direction=\"row\" align=\"center\" justify=\"between\">\n        <Text weight=\"bold\">{`$${price}`}</Text>\n\n        <CheckBox id={_id} name={_id} type={type} color=\"#8E677A\" />\n      </Box>\n    </Card>\n  );\n};\n\nexport default OfferCard;\n","/Users/mahmedtan/Developer/derm-app/src/pages/Book/Book.js",["471","472"],"import {\n  Box,\n  Heading,\n  Form,\n  Text,\n  ResponsiveContext,\n  Paragraph,\n} from \"grommet\";\nimport React, { useContext, useEffect, useRef } from \"react\";\n\nimport StepFooter from \"./StepFooter\";\nimport { useHistory } from \"react-router-dom\";\nimport Steps from \"./Steps/Steps\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport StepHeader from \"./StepHeader\";\n\nimport Loading from \"../Extras/Loading\";\nimport { changeValues } from \"../../reducers/formValuesReducer\";\nimport { uploadImages } from \"../../services/forms\";\nimport { useState } from \"react\";\nimport Spinner from \"../../components/Utils/Spinner\";\n\nconst Book = () => {\n  const size = useContext(ResponsiveContext);\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const [loading, setLoading] = useState(false);\n\n  const {\n    activeIndex,\n    procedures,\n    consultations,\n    formValues,\n    images,\n  } = useSelector(\n    ({ procedures, consultations, activeIndex, formValues, images }) => ({\n      activeIndex,\n      formValues,\n      procedures,\n      consultations,\n      images,\n    })\n  );\n\n  if (!(procedures && consultations && images)) {\n    return <Loading />;\n  }\n  if (loading)\n    return (\n      <Box\n        align=\"center\"\n        justify=\"center\"\n        height={{ min: \"100vh\" }}\n        pad=\"large\"\n      >\n        <Heading textAlign=\"center\">Processing your application</Heading>\n        <Box height=\"medium\">\n          <Spinner />\n        </Box>\n        <Paragraph>\n          This might take a minute or two depending on the number of images\n          you've uploaded. Meanwhile, Please do not refresh or close this page.\n        </Paragraph>\n      </Box>\n    );\n\n  return (\n    <Box align=\"center\" height={{ min: \"100vh\" }} margin={{ bottom: \"xlarge\" }}>\n      <Box\n        align=\"center\"\n        width=\"large\"\n        pad={size === \"small\" ? \"large\" : \"medium\"}\n        gap=\"medium\"\n      >\n        <Box gap=\"small\" fill>\n          <StepHeader />\n          <Heading margin={{ vertical: \"none\" }} level=\"2\" textAlign=\"center\">\n            {Steps[activeIndex].title}\n          </Heading>\n          <Text textAlign=\"center\">{Steps[activeIndex].description}</Text>\n        </Box>\n\n        <Box>\n          <Form\n            validate=\"blur\"\n            value={formValues}\n            onChange={(nextValue) => dispatch(changeValues(nextValue))}\n            onSubmit={({ value }) => {\n              setLoading(true);\n              images\n                ? uploadImages(images).then((res) => {\n                    window.sessionStorage.setItem(\n                      \"images\",\n                      JSON.stringify(res)\n                    );\n                    history.push(\"/processing\");\n                  })\n                : history.push(\"/processing\");\n            }}\n          >\n            <Box\n              gap=\"small\"\n              margin={{\n                bottom: \"xlarge\",\n                top: size === \"small\" ? \"large\" : \"none\",\n              }}\n            >\n              {Steps[activeIndex].item}\n              <StepFooter />\n            </Box>\n          </Form>\n        </Box>\n      </Box>\n    </Box>\n  );\n};\n\nexport default Book;\n","/Users/mahmedtan/Developer/derm-app/src/components/Header/Header.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Book/Processing.js",["473","474"],"import { useAuth0 } from