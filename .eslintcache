act, { useContext } from \"react\";\nimport { FormPrevious } from \"grommet-icons\";\nimport { Link } from \"react-router-dom\";\nimport Steps from \"./Steps/Steps\";\n\nimport { Box, Heading, Button, Text, ResponsiveContext } from \"grommet\";\n\nimport { decrementIndex } from \"../../reducers/indexReducer\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\nconst StepHeader = () => {\n  const dispatch = useDispatch();\n  const size = useContext(ResponsiveContext);\n  const activeIndex = useSelector(({ activeIndex }) => activeIndex);\n\n  return (\n    <Box gap=\"large\" align=\"start\">\n      {size === \"small\" &&\n        (activeIndex !== 0 ? (\n          <Button\n            icon={<FormPrevious />}\n            label=\"Back\"\n            onClick={() => dispatch(decrementIndex())}\n          />\n        ) : (\n          <Link to=\"/services\">\n            <Button\n              icon={<FormPrevious />}\n              size=\"small\"\n              label=\"Back to Services\"\n            />\n          </Link>\n        ))}\n      <Box\n        direction=\"row\"\n        fill=\"horizontal\"\n        align=\"center\"\n        justify={size === \"small\" ? \"center\" : \"between\"}\n      >\n        {size !== \"small\" &&\n          (activeIndex !== 0 ? (\n            <Button\n              icon={<FormPrevious />}\n              label=\"Back\"\n              onClick={() => dispatch(decrementIndex())}\n            />\n          ) : (\n            <Link to=\"/services\">\n              <Button\n                icon={<FormPrevious />}\n                size=\"small\"\n                label=\"Back to Services\"\n              />\n            </Link>\n          ))}\n        <Text textAlign=\"center\" weight=\"normal\">{`Step ${activeIndex + 1} of ${\n          Steps.length\n        }`}</Text>\n      </Box>\n    </Box>\n  );\n};\n\nexport default StepHeader;\n",["237","238"],"/Users/mahmedtan/Developer/derm-app/src/components/Slider/Slider.js",["239","240"],"im