  .filter((x) => formValues[x])\n        .indexOf(_id) >= 0\n    );\n  }, [formValues]);\n\n  return (\n    <Card\n      pad={size === \"small\" ? \"large\" : \"medium\"}\n      justify=\"center\"\n      border={selected && { color: \"#8E677A\", size: \"small\" }}\n      elevation={selected ? \"large\" : \"small\"}\n      round={size === \"small\" ? \"medium\" : \"small\"}\n    >\n      <Box direction=\"row\" justify=\"between\">\n        <Text size=\"large\">{title}</Text>\n        <Text>\n          <em>{`${time} min`}</em>\n        </Text>\n      </Box>\n\n      <Box height=\"xsmall\" overflow=\"hidden\">\n        <Paragraph>{description}</Paragraph>\n      </Box>\n\n      <Box direction=\"row\" align=\"center\" justify=\"between\">\n        <Text weight=\"bold\">{`$${price}`}</Text>\n\n        <CheckBox id={_id} name={_id} type={type} color=\"#8E677A\" />\n      </Box>\n    </Card>\n  );\n};\n\nexport default OfferCard;\n","/Users/mahmedtan/Developer/derm-app/src/pages/Book/Book.js",["476","477"],"import {\n  Box,\n  Heading,\n  Form,\n  Text,\n  ResponsiveContext,\n  Paragraph,\n} from \"grommet\";\nimport React, { useContext, useEffect, useRef } from \"react\";\n\nimport StepFooter from \"./StepFooter\";\nimport { useHistory } from \"react-router-dom\";\nimport Steps from \"./Steps/Steps\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport StepHeader from \"./StepHeader\";\n\nimport Loading from \"../Extras/Loading\";\nimport { changeValues } from \"../../reducers/formValuesReducer\";\nimport { uploadImages } from \"../../services/forms\";\nimport { useState } from \"react\";\nimport Spinner from \"../../components/Utils/Spinner\";\n\nconst Book = () => {\n  const size = useContext(ResponsiveContext);\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const [loading, setLoading] = useState(false);\n\n  const {\n    activeIndex,\n    procedures,\n    consultations,\n    formValues,\n    images,\n  } = useSelector(\n    ({ procedures, consultations, activeIndex, formValues, images }) => ({\n      activeIndex,\n      formValues,\n      procedures,\n      consultations,\n      images,\n    })\n  );\n\n  if (!(procedures && consultations && images)) {\n    return <Loading />;\n  }\n  if (loading)\n    return (\n      <Box\n        align=\"center\"\n        justify=\"center\"\n        height={{ min: \"100vh\" }}\n        pad=\"large\"\n      >\n        <Heading textAlign=\"center\">Processing your application</Heading>\n        <Box height=\"medium\">\n          <Spinner />\n        </Box>\n        <Paragraph>\n          This might take a minute or two depending on the number of images\n          you've uploaded. Meanwhile, Please do not refresh or close this page.\n        </Paragraph>\n      </Box>\n    );\n\n  return (\n    <Box align=\"center\" height={{ min: \"100vh\" }} margin={{ bottom: \"xlarge\" }}>\n      <Box\n        align=\"center\"\n        width=\"large\"\n        pad={size === \"small\" ? \"large\" : \"medium\"}\n        gap=\"medium\"\n      >\n        <Box gap=\"small\" fill>\n          <StepHeader />\n          <Heading margin={{ vertical: \"none\" }} level=\"2\" textAlign=\"center\">\n            {Steps[activeIndex].title}\n          </Heading>\n          <Text textAlign=\"center\">{Steps[activeIndex].description}</Text>\n        </Box>\n\n        <Box>\n          <Form\n            validate=\"blur\"\n            value={formValues}\n            onChange={(nextValue) => dispatch(changeValues(nextValue))}\n            onSubmit={({ value }) => {\n              setLoading(true);\n              images\n                ? uploadImages(images).then((res) => {\n                    window.sessionStorage.setItem(\n                      \"images\",\n                      JSON.stringify(res)\n                    );\n                    history.push(\"/processing\");\n                  })\n                : history.push(\"/processing\");\n            }}\n          >\n            <Box\n