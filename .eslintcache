barServices from \"../../components/Sidebars/SidebarServices\";\n\nconst Services = () => {\n  const size = useContext(ResponsiveContext);\n  const [services, serviceTypes] = useSelector(({ services, serviceTypes }) => [\n    services,\n    serviceTypes,\n  ]);\n  const match = useRouteMatch(\"/services/:slug\");\n  const service =\n    match && services\n      ? services.find((service) => service.slug === match.params.slug)\n      : undefined;\n\n  if (!serviceTypes) return <Loading />;\n\n  return (\n    <Box>\n      <Header />\n      <Box direction=\"row-responsive\" justify=\"end\">\n        <Box\n          width=\"100%\"\n          height=\"88vh\"\n          justify=\"end\"\n          align=\"center\"\n          overflow=\"auto\"\n        >\n          <Switch>\n            <Route path=\"/services/:slug\">\n              <Service\n                service={service}\n                services={services}\n                serviceTypes={serviceTypes}\n                size={size}\n              />\n            </Route>\n          </Switch>\n        </Box>\n        {size !== \"small\" && size !== \"xsmall\" ? (\n          <SidebarServices serviceTypes={serviceTypes} />\n        ) : null}\n      </Box>\n    </Box>\n  );\n};\n\nexport default Services;\n","/Users/mahmedtan/Developer/derm-app/src/components/Authentication/AuthenticationSnackBar.js",["462"],"/Users/mahmedtan/Developer/derm-app/src/components/Authentication/AuthenticationSnackbar.js",["463"],"import { useAuth0 } from \"@auth0/auth0-react\";\nimport { Box, Button } from \"grommet\";\nimport React from \"react\";\nimport LoginBtn from \"./LoginBtn\";\nimport { Link } from \"react-router-dom\";\nimport { User } from \"grommet-icons\";\n\nconst AutheticationSnackbar = () => {\n  const { isAuthenticated } = useAuth0();\n\n  return isAuthenticated ? (\n    <Link to=\"/profile\">\n      <Button icon={<User />} />\n    </Link>\n  ) : (\n    <LoginBtn primary />\n  );\n};\n\nexport default AutheticationSnackbar;\n",[