patch({ type: \"RESET_IMAGES\" });\n            dispatch({ type: \"RESET_DATE\" });\n            setError(\"Sorry for inconvenience\");\n          });\n      }\n      if (formValues.emailAddress === \"\")\n        setError(\"Looks like you're on the wrong page\");\n    }\n  }, [formValues, procedures, consultations, date, dispatch, submitted]);\n  useEffect(() => {\n    if (submitted) {\n      window.sessionStorage.clear();\n      window.sessionStorage.setItem(\n        \"fullName\",\n        formValues.firstName + \" \" + formValues.lastName\n      );\n      window.sessionStorage.setItem(\"submittedDate\", date);\n\n      dispatch({ type: \"RESET_FORM\" });\n      dispatch({ type: \"RESET_INDEX\" });\n      dispatch({ type: \"RESET_IMAGES\" });\n      dispatch({ type: \"RESET_DATE\" });\n\n      history.push(\"/confirmation\");\n    }\n  }, [submitted, dispatch]);\n\n  if (error)\n    return (\n      <Layout>\n        <NotFound />\n      </Layout>\n    );\n  if (!submitted) return <Loading />;\n  return \"Yes\";\n};\n\nexport default Processing;\n","/Users/mahmedtan/Developer/derm-app/src/reducers/formValuesReducer.js",[],"/Users/mahmedtan/Developer/derm-app/src/reducers/dateReducer.js",[],["478","479"],"/Users/mahmedtan/Developer/derm-app/src/reducers/imageReducer.js",[],["480","481"],"/Users/mahmedtan/Developer/derm-app/src/pages/Home.js",[],"/Users/mahmedtan/Developer/derm-app/src/components/StoryCards/StoryCard.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Book/Confirmation.js",["482"],"import { Box, Heading, Card, Paragraph, Button } from \"grommet\";\nimport React, { useEffect, useState } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport Layout from \"../../components/Utils/Layout\";\nimport NotFound from \"../Extras/404\";\nimport Loading from \"../Extras/Loading\";\n\nconst Confirmation = () => {\n  const [fullName, setFullName] = useState(null);\n  const [submittedDate, setSubmittedDate] = useState(null);\n  const [error, setError] = useState(false);\n\n  useEffect(() => {\n    if (\n      window.sessionStorage.getItem(\"fullName\") &&\n      window.sessionStorage.getItem(\"submittedDate\")\n    ) {\n      setFullName(window.sessionStorage.getItem(\"fullName\"));\n      setSubmittedDate(window.sessionStorage.getItem(\"submittedDate\"));\n    } else setError(true);\n  }, []);\n  if (error)\n    return (\n      <Layout>\n        <NotFound />\n      </Layout>\n    );\n\n  return (\n    <Layout>\n      <Box\n        align=\"center\"\n        justify=\"start\"\n        height={{ min: \"100vh\" }}\n        margin=\"large\"\n      >\n        <Heading>You are all set!</Heading>\n        <Card align=\"center\">\n          <Box pad=\"medium\" align=\"center\" gap=\"medium\">\n            <Heading level=\"3\" margin=\"none\">\n              Dear {fullName}\n            </Heading>\n            <Paragraph size=\"large\" textAlign=\"center\">\n              Your appointment has been booked for{\" \"}\n              {new Date(submittedDate).toLocaleDateString(\"en-US\", {\n                year: \"numeric\",\n                month: \"long\",\n                day: \"numeric\",\n              }) +\n                \" \" +\n                new Date(submittedDate).toLocaleTimeString(\"en-US\", {\n                  timeStyle: \"short\",\n                })}\n            </Paragraph>\n            <Link to=\"/profile\">\n              <Button primary label=\"Go to profile\" />\n            </Link>\n          </Box>\n        </Card>\n      </Box>\n    </Layout>\n  );\n};\n\nexport default Confirmation;\n","/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Profile.js",["483","484"],"/Users/mahmedtan/Developer/derm-app/src/services/forms.js",[],"/Users/mahmedtan/Developer/derm-app/src/pages/Profile/Appointments/Consult.js",[],"/Users/mahmedtan/Developer/derm-app/src/auth/auth0-provider-with-history.js",[],"/Users/mahmedtan/Developer/derm-app/src/services/services.js",[],"/Users/mahmedtan/Developer/derm-app/src/pa